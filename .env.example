# Firebase Configuration
# Get your credentials from: https://console.firebase.google.com/
NEXT_PUBLIC_FIREBASE_API_KEY=your_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id

# Optional: Firebase Measurement ID for Analytics
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Site Configuration (Optional - overrides config/site.ts)
NEXT_PUBLIC_SITE_URL=https://neezar.tech
NEXT_PUBLIC_SITE_NAME=Neezar Abdurrahman

# Environment
NODE_ENV=development
